    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9"
  },
  "engines": {
    "node": "10.x"
  }
}